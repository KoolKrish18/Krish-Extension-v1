{
  "manifest_version": 2,
  "name": "KrishExtensionv1",
  "description": "Add a custom background to Google.com",
  "version": "1.0",
  "icons": {
    "16": "/images/KN-logo-16.png",
    "48": "/images/KN-logo-48.png",
    "128": "/images/KN-logo-128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "/images/KN-logo-16(2).png",
      "48": "/images/KN-logo-48(2).png",
      "128": "/images/KN-logo-128(2).png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "css": ["main.css"]
    }
  ]
}
